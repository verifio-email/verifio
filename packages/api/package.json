		"format": "prettier --check . --ignore-path ../../.gitignore",
		"lint": "eslint",
		"typecheck": "tsc --build --noEmit --emitDeclarationOnly false"
	},
	"dependencies": {
		"@elysiajs/eden": "catalog:backend"
	},
	"devDependencies": {
		"@reloop/tsconfig": "workspace:*",
		"@types/node": "catalog:",
		"typescript": "catalog:"
	}
}
