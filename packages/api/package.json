{
	"name": "@reloop/api",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"exports": {
		".": "./src/index.ts",
		"./auth": "./src/auth.ts",
		"./email": "./src/email.ts",
		"./types": "./src/types.ts"
	},
	"scripts": {
		"build": "tsc --build",
		"clean": "git clean -xdf .cache .turbo dist node_modules",
