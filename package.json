	"scripts": {
		"fe:dev:dev": "bun run --filter=dev dev",
		"fe:dev:build": "bun run --filter=dev build",
		"fe:dev:start": "bun run --filter=dev start",
		"fe:docs:dev": "bun run --filter=docs dev",
		"fe:docs:build": "bun run --filter=docs build",
		"fe:docs:start": "bun run --filter=docs start",
