		"be:api-key:start": "bun run --filter=api-key start",
		"be:workflow:dev": "bun run --filter=workflow dev",
		"be:workflow:build": "bun run --filter=workflow build",
		"be:workflow:start": "bun run --filter=workflow start",
		"be:tracehub:dev": "bun run --filter=tracehub dev",
		"be:tracehub:build": "bun run --filter=tracehub build",
		"be:tracehub:start": "bun run --filter=tracehub start",
		"be:template:dev": "bun run --filter=template dev",
		"be:template:build": "bun run --filter=template build",
		"be:template:start": "bun run --filter=template start",
		"be:upload:dev": "bun run --filter=upload dev",
		"be:upload:build": "bun run --filter=upload build",
		"be:upload:start": "bun run --filter=upload start",
		"check": "biome check --write .",
		"frontend:dev": "turbo dev --filter='dashboard' --filter='dev' --filter='docs' --filter='web'",
		"backend:dev": "turbo dev --filter='contacts' --filter='auth' --filter='domain' --filter='mail' --filter='webhook' --filter='api-key' --filter='workflow' --filter='tracehub' --filter='upload' --filter='template'",
		"reloop:dev": "turbo dev --filter='dashboard' --filter='dev' --filter='docs' --filter='web' --filter='contacts' --filter='auth' --filter='domain' --filter='mail' --filter='webhook' --filter='api-key' --filter='workflow' --filter='tracehub' --filter='upload' --filter='template'",
		"docker:up": "docker compose -f local/docker-compose.yml up -d",
		"docker:down": "docker compose -f local/docker-compose.yml down",
		"docker-prod:up": "docker compose -f prod/docker-compose.yml up -d",
		"docker-prod:down": "docker compose -f prod/docker-compose.yml down",
		"docker-prod:restart": "docker compose -f prod/docker-compose.yml restart",
		"docker-prod:logs": "docker compose -f prod/docker-compose.yml logs -f",
		"docker-prod:ps": "docker compose -f prod/docker-compose.yml ps",
		"db:push": "bun run --filter=@reloop/db push",
		"db:studio": "bun run --filter=@reloop/db studio",
		"db:generate": "bun run --filter=@reloop/db generate",
		"db:migrate": "bun run --filter=@reloop/db migrate",
		"db:seed": "bun run --filter=@reloop/db seed"
