{
	"extends": "@verifio/tsconfig/nextjs.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@fe/dashboard/*": ["./src/*"],
			"@be/domain/*": ["../../backend/domain/src/*"],
			"@be/workflow/*": ["../../backend/workflow/src/*"]
		}
	},
	"include": [
		"./**/*.ts",
		"./**/*.tsx",
		"./.next/types/**/*.ts",
		"./next-env.d.ts",
		".next/types/**/*.ts"
	],
	"exclude": ["./node_modules"]
}
